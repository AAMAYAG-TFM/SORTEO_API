pipeline {
    agent any

    stages {
        stage('Source') {
            steps {
                git 'https://github.com/AAMAYAG-TFM/SORTEO_API.git'
            }
            
            stage('Building our image') {
steps{
script {
dockerImage = docker.build registry + ":$BUILD_NUMBER"
}
}
}
        }
        

    }
  }
